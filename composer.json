{
    "name": "martin-kozianka/contao-webdav",
    "description": "Webdav for contao based on sabre/dav",
    "keywords": [
        "contao",
        "sabre",
        "sabredav",
        "carddav",
        "caldav",
        "webdav"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Martin Kozianka",
            "email": "martin@kozianka.de",
            "homepage": "http://kozianka.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "martin@kozianka.de",
        "issues": "https://github.com/kozi/contao-webdav/issues",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/kozi/contao-webdav"
    },
    "autoload": {
        "psr-0": {
            "ContaoWebdav": "src/"
        }
    },
    "require": {
        "php": ">=5.4",
        "contao/core": ">=2.11,<3.4",
        "contao-community-alliance/composer-plugin": "~2.0",
        "sabre/dav": ">=2,<3"
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/webdav"
            }
        }
    },
    "config": {
        "component-dir": "../assets/components"
    }
}
