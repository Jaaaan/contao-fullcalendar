{
    "name": "martin-kozianka/contao-fullcalendar",
    "description": "FullCalendar, http://arshaw.com/fullcalendar for Contao",
    "keywords": [
        "contao",
        "calendar",
        "fullcalendar",
        "ical",
        "ics"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Martin Kozianka",
            "email": "martin@kozianka.de",
            "homepage": "http://kozianka.de",
            "role": "Developer"
        }
    ],  
    "support": {
        "email": "martin@kozianka.de",
        "issues": "https://github.com/kozi/contao-fullcalendar/issues",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/kozi/contao-fullcalendar"
    },
    "autoload": {
        "psr-0": {
            "ContaoFullcalendar": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/iCalcreator/iCalcreator"
        }
    ],
    "require": {
        "php": ">=5.4",
        "contao/core": "~3.3",
        "contao-community-alliance/composer-plugin": "~2.2",
        "kigkonsult/icalcreator": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/fullcalendar"
            }
        }
    },
    "config": {
        "component-dir": "../assets/components"
    }
}
